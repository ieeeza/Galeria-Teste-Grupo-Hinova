{
  "expo": {
    "name": "Galeria de fotos Hinova",
    "slug": "galeria-hinova",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/simbolo-hinova.png",
    "scheme": "galeriahinova",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Permita que o aplicativo acesse a câmera do dispositivo.",
        "NSLocationWhenInUseUsageDescription": "Permita que o aplicativo acesse sua localização enquanto estiver em uso.",
        "NSLocationAlwaysUsageDescription": "Permita que o aplicativo acesse sua localização mesmo em segundo plano."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/simbolo-hinova.png",
        "backgroundColor": "transparent"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ieza.galeriahinova"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/simbolo-hinova.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/simbolo-hinova.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita que o aplicativo acesse sua localização mesmo em segundo plano."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "24ba317d-e290-470d-94f0-d537ae2661d9"
      }
    }
  }
}
